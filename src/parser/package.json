{
  "name": "@gml-modules/parser",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./gml-parser.js": "./dist/gml-parser.js",
    "./generated/*": "./dist/generated/*",
    "./src/*": "./dist/src/*",
    "./utils/recognition-exception.js": "./dist/src/utils/recognition-exception.js"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "antlr4": "^4.13.2",
    "@gml-modules/core": "0.0.1",
    "@gml-modules/semantic": "0.0.1"
  },
  "engines": {
    "node": ">=25.0.0"
  },
  "scripts": {
    "build:types": "tsc -p tsconfig.json",
    "clean:types": "rm -rf dist",
    "test": "npm run build:types && node --test dist/test/*.test.js",
    "antlr": "java -Xmx500M -jar ../../resources/antlr/antlr4-4.13.2-complete.jar -Dlanguage=JavaScript -visitor -o generated/ GameMakerLanguageLexer.g4 && java -Xmx500M -jar ../../resources/antlr/antlr4-4.13.2-complete.jar -Dlanguage=JavaScript -visitor -o generated/ GameMakerLanguageParser.g4"
  }
}
