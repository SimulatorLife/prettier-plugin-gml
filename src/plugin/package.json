{
  "name": "prettier-plugin-gamemaker",
  "type": "module",
  "exports": {
    ".": "./src/gml.js",
    "./project-index": "./src/project-index.js",
    "./identifier-case": "./src/identifier-case.js"
  },
  "config": {
    "root_path": "."
  },
  "scripts": {
    "test": "node --test test/*.test.js",
    "prettier:plugin:raw": "npx prettier $npm_config_path --plugin=$npm_package_config_root_path/src/gml.js --ignore-path=$npm_package_config_root_path/../cli/.prettierignore --ignore-unknown --log-level=warn --no-error-on-unmatched-pattern",
    "example": "npm run prettier:plugin:raw --path=test/test14.input.gml",
    "prettier:plugin": "node ../cli/src/cli.js $npm_config_path"
  },
  "dependencies": {
    "@prettier-plugin-gml/shared": "file:../shared",
    "gamemaker-language-parser": "file:../parser",
    "gamemaker-language-semantic": "file:../semantic"
  },
  "engines": {
    "node": ">=25.0.0"
  },
  "peerDependencies": {
    "prettier": "^3.0.0"
  }
}
