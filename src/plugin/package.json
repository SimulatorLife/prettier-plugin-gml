{
  "name": "prettier-plugin-gamemaker",
  "main": "src/gml.js",
  "type": "module",
  "scripts": {
    "example": "prettier --plugin ./src/gml.js example.gml",
    "write": "prettier --plugin=./src/gml.js --write $npm_config_path --ignore-path .prettierignore --no-error-on-unmatched-pattern --loglevel=warn"
  },
  "dependencies": {
    "gamemaker-language-parser": "0.0.1",
    "prettier": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.16.3"
  }
}