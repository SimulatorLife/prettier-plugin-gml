{
  "name": "@gml-modules/plugin",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": "./dist/src/index.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "config": {
    "root_path": "."
  },
  "scripts": {
    "build:types": "tsc -p tsconfig.json",
    "clean:types": "rm -rf dist",
    "test": "npm run build:types && node --test dist/test/*.test.js",
    "prettier:plugin:raw": "npm run build:types && npx prettier $npm_config_path --plugin=$npm_package_config_root_path/dist/src/plugin-entry.js --ignore-path=$npm_package_config_root_path/../cli/.prettierignore --ignore-unknown --log-level=warn --no-error-on-unmatched-pattern",
    "example": "npm run prettier:plugin:raw --path=test/test14.input.gml",
    "prettier:plugin": "npm run build:types && npm run build:types --workspace=@gml-modules/cli && node ../cli/dist/index.js $npm_config_path"
  },
  "dependencies": {
    "@gml-modules/core": "0.0.1",
    "@gml-modules/parser": "^0.0.1",
    "@gml-modules/semantic": "0.0.1"
  },
  "engines": {
    "node": ">=25.0.0"
  },
  "peerDependencies": {
    "prettier": "^3.0.0"
  }
}
