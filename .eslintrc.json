{
    "env": {
        "browser": true,
        "es2022": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 2022,
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error", 4, { "SwitchCase": 1 }],
        "quotes": ["warn", "double"],
        "semi": ["error", "always"],
        "no-unused-vars": ["warn"],
        "no-console": ["off"],
        "comma-dangle": ["error", "never"]
    }
  }
  