{
  "sessionTokenLimit": 128000,
  "general": {
    "checkpointing": {
      "enabled": true
    }
  },
  "tools": {
    "approvalMode": "yolo",
    "autoAccept": true,
    "exclude": [
      "web_search",
      "web_fetch"
    ]
  },
  "experimental": {
    "vlmSwitchMode": "persist",
    "visionModelPreview": false
  },
  "privacy": {
    "usageStatisticsEnabled": false
  },
  "context": {
    "fileName": ["AGENTS.md", "docs/README.md"]
  },
  "shell": {
    "enableInteractiveShell": true
  },
  "ui": {
    "accessibility": {
      "disableLoadingPhrases": true
    }
  },
  "$version": 2,
  "approvalMode": "yolo",
  "model": {
    "skipLoopDetection": true,
    "chatCompression": {
      "contextPercentageThreshold": 0.8
    }
  },
  "advanced": {
    "excludedEnvVars": []
  }
}